{
  "name": "@progress/kendo-vue-listview",
  "version": "3.13.0",
  "description": "Vue ListView enables you to display a custom layout of data items. Kendo UI for Vue ListView package",
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-vue.git"
  },
  "main": "dist/npm/main.js",
  "types": "dist/npm/main.d.ts",
  "module": "dist/es/main.js",
  "jsnext:main": "dist/es/main.js",
  "exports": {
    "node": "./dist/esm/main.js",
    "import": "./dist/es/main.js",
    "require": "./dist/npm/main.js",
    "default": "./dist/npm/main.js"
  },
  "scripts": {
    "build-package": "gulp build-package",
    "start": "gulp start --notsc",
    "test": "cd ../../ && npm run test -- --testPathPattern=/packages/listview/.*",
    "e2e": "cd ../../ && npx jest --maxWorkers=4 --config jest.config.e2e.js packages/listview"
  },
  "homepage": "https://www.telerik.com/kendo-vue-ui",
  "keywords": [
    "Kendo UI",
    "Vue",
    "Progress",
    "ListView",
    "Kendo UI for Vue",
    "vuejs",
    "UI",
    "components",
    "Vue component",
    "Telerik"
  ],
  "peerDependencies": {
    "@progress/kendo-licensing": "^1.3.0",
    "vue": "^2.6.12 || ^3.0.2"
  },
  "dependencies": {
    "@progress/kendo-vue-common": "3.13.0"
  },
  "devDependencies": {
    "@progress/kendo-data-query": "^1.0.0",
    "@progress/kendo-date-math": "^1.4.1",
    "@progress/kendo-drawing": "^1.8.0",
    "@progress/kendo-licensing": "^1.3.0",
    "@progress/kendo-vue-buttons": "3.13.0",
    "@progress/kendo-vue-data-tools": "3.13.0",
    "@progress/kendo-vue-dateinputs": "3.13.0",
    "@progress/kendo-vue-dropdowns": "3.13.0",
    "@progress/kendo-vue-inputs": "3.13.0",
    "@progress/kendo-vue-intl": "3.13.0",
    "@progress/kendo-vue-layout": "3.13.0"
  },
  "@progress": {
    "friendlyName": "ListView",
    "framework": "Kendo UI for Vue"
  },
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
